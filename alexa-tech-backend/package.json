{
  "name": "alexa-tech-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:seed": "ts-node prisma/seed.ts",
    "test:clients": "node scripts/test-clients-module.js",
    "test:users": "npx prisma migrate reset --force && node scripts/test-users-module.js",
    "test:productos": "node scripts/test-products-module.js",
    "test:all": "node scripts/run-all-tests.js",
    "check:auth": "node scripts/check-auth-health.js",
    "check:auth:admin": "set AUTH_EMAIL=admin@alexatech.com&& set AUTH_PASSWORD=admin123&& node scripts/check-auth-health.js",
    "check:auth:supervisor": "set AUTH_EMAIL=supervisor@alexatech.com&& set AUTH_PASSWORD=supervisor123&& node scripts/check-auth-health.js",
    "test:entidades:admin": "set AUTH_EMAIL=admin@alexatech.com&& set AUTH_PASSWORD=admin123&& node scripts/test-clients-module.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "alexa-tech",
    "backend",
    "api",
    "typescript"
  ],
  "author": "Alexa Tech Team",
  "license": "MIT",
  "description": "Backend API para el sistema de gesti√≥n empresarial Alexa Tech",
  "dependencies": {
    "@alexa-tech/shared": "file:../shared",
    "@prisma/client": "^6.16.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.1"
  }
}
